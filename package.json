{
  "name": "patternfly-org-workspace",
  "private": true,
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "analyze": "yarn workspace patternfly-org-5 analyze",
    "develop": "yarn develop:pf-site",
    "develop:pf-site": "yarn workspace patternfly-org-5 develop",
    "build": "yarn build:pf-site",
    "build:analyze": "yarn workspace patternfly-org-5 build:analyze && yarn copy:pf-site",
    "build:pf-site": "yarn workspace patternfly-org-5 build && yarn copy:pf-site",
    "copy:pf-site": "rm -rf build/patternfly-org/pf-site && mkdir -p build/patternfly-org && cp -r packages/patternfly-site/public build/patternfly-org/patternfly-site && cp -r build/patternfly-org/patternfly-site/assets build/patternfly-org/assets",
    "clean": "lerna run clean && rm -rf build",
    "serve": "pf-docs-framework serve build/patternfly-org",
    "test:pf-site": "yarn workspace patternfly-org-5 test:a11y:ci",
    "screenshots": "yarn workspace patternfly-org-5 screenshots",
    "start": "yarn start:pf-site",
    "start:pf-site": "yarn develop:pf-site",
    "uninstall": "find . -name node_modules | xargs rm -rf",
    "workflows": "node .github/generate-workflows",
    "test:a11y": "yarn workspace patternfly-org-5 test:a11y"
  },
  "workspaces": {
    "packages": [
      "packages/ast-helpers",
      "packages/documentation-framework",
      "packages/v4"
    ]
  },
  "devDependencies": {
    "@octokit/rest": "^19.0.7",
    "glob": "^8.1.0",
    "lerna": "^6.4.1",
    "surge": "^0.23.1"
  },
  "resolutions": {
    "node-sass": ">=6.0.1"
  }
}
